{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "blocks"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Excerpt": {
      "type": "text"
    },
    "Post_Status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ]
    },
    "Publish_Date": {
      "type": "datetime"
    },
    "post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post.post",
      "inversedBy": "Author"
    },
    "Author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::post.post",
      "mappedBy": "post"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "Posts"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "Posts"
    }
  }
}
